@model MvcAdmin.ViewModels.CourseViewModel

<h1>Här är information om @Model.CourseTitle-kursen</h1>
<p>Title: @Model.CourseTitle </p>
<p>Kursnummer: @Model.CourseNumber</p>
<p>Beskrivning: @Model.CourseDescription</p>
<p>Detaljer: @Model.CourseDetails</p>
<p><a asp-controller="Course" asp-action="DeleteCourse" asp-route-id="@Model.CourseId">Ta bort</a> <a asp-controller="Course" asp-action="EditCourse" asp-route-id="@Model.CourseId">Uppdatera</a></p>